{
  "name": "polymer-starter-kit",
  "version": "4.0.0",
  "description": "A starting point for Polymer apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halkeye/critshow-bingo.git"
  },
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halkeye/critshow-bingo/issues"
  },
  "disabled-main": "dist/critshow-bingo-app.js",
  "disabled-module": "dist/critshow-bingo-app.js",
  "disabled-type": "module",
  "homepage": "https://github.com/halkeye/critshow-bingo#readme",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf build",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat my-element.bundled.js | gzip -9 | wc -c ; rm my-element.bundled.js"
  },
  "dependencies": {
    "@polymer/paper-button": "^3.0.1",
    "lit-element": "^2.3.1",
    "nanoid": "^3.1.12",
    "pwa-helpers": "^0.9.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@open-wc/rollup-plugin-html": "^1.1.0",
    "@open-wc/testing": "^2.5.19",
    "@open-wc/testing-karma": "^4.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/chai": "^4.2.12",
    "@types/js-yaml": "^3.12.5",
    "@types/karma": "^5.0.0",
    "@types/mocha": "^8.0.1",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.57.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-lit": "^1.2.2",
    "karma": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-eslint": "^2.2.0",
    "karma-mocha": "^2.0.1",
    "lit-analyzer": "^1.2.1",
    "mocha": "^8.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.7",
    "web-component-analyzer": "^1.1.6"
  },
  "engines": {
    "node": ">=8.0"
  }
}
